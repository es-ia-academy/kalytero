      <!--CSS-->
      <style id="UoAajlY" type="text/css" data-type="css" data-size="laptop" data-content="section">grid-container#yiPqTYa {
  position: relative;
}

grid-box#xeEmBst {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(auto, 200px));
  grid-auto-rows: auto;
  gap: 10px;
  width: 100%;
  height: 100%;
  padding: 3%;
  place-content: center;
}

grid-bg#hhvOlBM {
  position: absolute;
  height: 100%;
  width: 100%;
  z-index: -1;
  display: block;
}

div#cIoViDN,
div#OonuAFP,
div#UzyVPgn,
div#LHfUWDo {
  height: 100%;
  width: 100%;
}

div#KRLMwHT {
  height: 100%;
  width: 100%;
}

div#iGrcdVf {
  height: 100%;
  width: 100%;
}

img#WEnmpKm {
  object-fit: cover;
  height: 100%;
  width: 100%;
}

video#gfaPGLs {
  object-fit: cover;
  height: 100%;
  width: 100%;
}

grid-box#xeEmBst grid-item {
  box-shadow: 0 20px 25px -5px rgb(0 0 0 / 10%), 0 10px 10px -5px rgb(0 0 0 / 4%);
  border-radius: 10px;
  overflow: hidden;
  background: white;
}

grid-box#xeEmBst item-body {
  padding: 15px;
  display: grid;
  gap: 10px;
}

grid-box#xeEmBst grid-title {
  font-size: 1.5rem;
  color: white;
}

grid-box#xeEmBst grid-desc {
  font-size: 12px;
  color: #929292;
}

grid-box#xeEmBst item-title {
  color: black;
}

grid-box#xeEmBst item-desc {
  display: grid;
  font-size: 12px;
  color: #929292;
}

grid-head#ChLKqpY {
  grid-column: 1 / -1;
  display: grid;
  margin-left: 15px;
}

grid-box#xeEmBst item-head img {
  background-color: black;
  height: 200px;
  width: 100%;
  object-fit: cover;
}
</style>
